{
  "compilerOptions": {
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "noImplicitAny": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "lib": ["es2018"],
      "module": "commonJS",
      "target": "es2018",
  },
  "include": [
      "**/*",
  ],
  "exclude": [
      "node_modules"
  ]
}